{
  "name": "Rumia Browser",
  "description": "Um browser simples que pode rodar em qualquer p√°gina!",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "32": "img/rumia.png"
    }
  },
  "background": {
    "service_worker": "js/touhou.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "/*"
      ],
      "matches": [
        "https://atendimento.frasanet.com.br/*"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "scripting"
  ]
}
